{
  "name": "vue-countup-plus",
  "type": "module",
  "version": "0.1.0",
  "private": false,
  "license": "MIT",
  "description": "A Vue 3 component that wraps CountUp.js to create animated number transitions",
  "author": "",
  "keywords": [
    "vue",
    "vue3",
    "countup",
    "animation",
    "typescript"
  ],
  "types": "./dist/index.d.ts",
  "main": "./dist/vue-countup-plus.umd.cjs",
  "module": "./dist/vue-countup-plus.js",
  "unpkg": "./dist/vue-countup-plus.iife.js",
  "jsdelivr": "./dist/vue-countup-plus.iife.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/vue-countup-plus.js",
      "require": "./dist/vue-countup-plus.umd.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "test": "vitest run",
    "prepublishOnly": "pnpm test && pnpm build",
    "build": "vite build && vue-tsc --declaration --emitDeclarationOnly",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write ."
  },
  "dependencies": {
    "countup.js": "^2.8.0",
    "vue": "^3.5.0"
  },
  "peerDependencies": {
    "vue": "^3.5.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.6.1",
    "@types/node": "^20.11.5",
    "@vitejs/plugin-vue": "^5.0.3",
    "@vue/test-utils": "^2.4.3",
    "@vue/tsconfig": "^0.5.1",
    "eslint": "^8.56.0",
    "jsdom": "^23.2.0",
    "prettier": "^3.2.4",
    "typescript": "^5.3.3",
    "vite": "^5.0.12",
    "vitepress": "^1.0.0-rc.40",
    "vitest": "^1.2.1",
    "vue-tsc": "^1.8.27"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  }
}